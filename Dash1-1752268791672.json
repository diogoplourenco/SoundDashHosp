          "orderByTime": "ASC",
          "policy": "default",
          "query": "import \"strings\"\r\n\r\n\r\nfrom(bucket: \"Sensor_Data\")\r\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r._measurement == \"${sensorName}\")\r\n  |> filter(fn: (r) =>\r\n    r._field == \"00025_Hz\" or\r\n    r._field == \"00031.5_Hz\" or\r\n    r._field == \"00040_Hz\" or\r\n    r._field == \"00050_Hz\" or\r\n    r._field == \"00063_Hz\" or\r\n    r._field == \"00080_Hz\" or\r\n    r._field == \"00100_Hz\" or\r\n    r._field == \"00125_Hz\" or\r\n    r._field == \"00160_Hz\" or\r\n    r._field == \"00200_Hz\" or\r\n    r._field == \"00250_Hz\" or\r\n    r._field == \"00315_Hz\" or\r\n    r._field == \"00400_Hz\" or\r\n    r._field == \"00500_Hz\" or\r\n    r._field == \"00630_Hz\" or\r\n    r._field == \"00800_Hz\" or\r\n    r._field == \"01000_Hz\" or\r\n    r._field == \"01250_Hz\" or\r\n    r._field == \"01600_Hz\" or\r\n    r._field == \"02500_Hz\" or\r\n    r._field == \"03150_Hz\" or\r\n    r._field == \"04000_Hz\" or\r\n    r._field == \"05000_Hz\" or\r\n    r._field == \"06300_Hz\" or\r\n    r._field == \"08000_Hz\" or\r\n    r._field == \"10000_Hz\" or\r\n    r._field == \"12500_Hz\" or\r\n    r._field == \"16000_Hz\" or\r\n    r._field == \"20000_Hz\"\r\n  )\r\n  |> group(columns: [\"_field\"])\r\n  |> last()\r\n  |> map(fn: (r) => {\r\n    freq_num = \r\n      if r._field == \"00025_Hz\" then \"001\"\r\n      else if r._field == \"00031.5_Hz\" then \"002\"\r\n      else if r._field == \"00040_Hz\" then \"003\"\r\n      else if r._field == \"00050_Hz\" then \"004\"\r\n      else if r._field == \"00063_Hz\" then \"005\"\r\n      else if r._field == \"00080_Hz\" then \"006\"\r\n      else if r._field == \"00100_Hz\" then \"007\"\r\n      else if r._field == \"00125_Hz\" then \"008\"\r\n      else if r._field == \"00160_Hz\" then \"009\"\r\n      else if r._field == \"00200_Hz\" then \"010\"\r\n      else if r._field == \"00250_Hz\" then \"011\"\r\n      else if r._field == \"00315_Hz\" then \"012\"\r\n      else if r._field == \"00400_Hz\" then \"013\"\r\n      else if r._field == \"00500_Hz\" then \"014\"\r\n      else if r._field == \"00630_Hz\" then \"015\"\r\n      else if r._field == \"00800_Hz\" then \"016\"\r\n      else if r._field == \"01000_Hz\" then \"017\"\r\n      else if r._field == \"01250_Hz\" then \"018\"\r\n      else if r._field == \"01600_Hz\" then \"019\"\r\n      else if r._field == \"02500_Hz\" then \"020\"\r\n      else if r._field == \"03150_Hz\" then \"021\"\r\n      else if r._field == \"04000_Hz\" then \"022\"\r\n      else if r._field == \"05000_Hz\" then \"023\"\r\n      else if r._field == \"06300_Hz\" then \"024\"\r\n      else if r._field == \"08000_Hz\" then \"025\"\r\n      else if r._field == \"10000_Hz\" then \"026\"\r\n      else if r._field == \"12500_Hz\" then \"027\"\r\n      else if r._field == \"16000_Hz\" then \"028\"\r\n      else if r._field == \"20000_Hz\" then \"029\"\r\n      else \"999\"\r\n\r\n    freq_label =\r\n      if r._field == \"00031.5_Hz\" then \"31.5\"\r\n      else string(v: int(v: strings.replaceAll(v: r._field, t: \"_Hz\", u: \"\")))\r\n\r\n    return {\r\n      freq_ordem: freq_num,\r\n      freq_label: freq_label,\r\n      _value: r._value\r\n    }\r\n  })\r\n  |> sort(columns: [\"freq_ordem\"])\r\n  |> keep(columns: [\"freq_label\",\"_value\"])",
